{
  "name": "@maximnara/queuework",
  "version": "1.0.3",
  "description": "Job manager for NodeJs",
  "main": "dist/index.js",
  "scripts": {
    "just_test": "jest",
    "test": "NODE_ENV=feature/travis jest --notify --coverage --coverageReporters=text-lcov | coveralls",
    "build": "babel src/ -d dist",
    "start": "npm run build && nodemon dist/index.js"
  },
  "author": "maximnara",
  "repository": {
    "type": "git",
    "url": "https://github.com/maximnara/queuework.git"
  },
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
    ],
    "testPathIgnorePatterns" : [
      "src/db/**/*.js",
      "src/index.js"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "cron": "^1.7.1",
    "dotenv": "^8.0.0",
    "mysql2": "^1.6.5",
    "redis": "^2.8.0",
    "sequelize": "^5.13.1",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "coveralls": "^3.0.6",
    "jest": "^24.8.0"
  }
}
